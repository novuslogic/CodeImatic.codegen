<?xml version="1.0" encoding="utf-8" ?>
<config version="codeimatic.codegen1.0">
    <plugins>
        <SysTags>
          <filename>SysTags.dll</filename>
        </SysTags>
         <CodeDocsTags>
          <filename>CodeDocsTags.dll</filename>
        </CodeDocsTags>
        <DBTags>
          <filename>DBTags.dll</filename>
        </DBTags>
        <JSONTags>
          <filename>JSONTags.dll</filename>
        </JSONTags>
        <XMLTags>
          <filename>XMLTags.dll</filename>
        </XMLTags>
        <SystemExt>
          <filename>CodeImatic.codegen.Pascal.SystemExt.dll</filename>
        </SystemExt>
        <WebProcessor>
           <filename>WebProcessor.dll</filename>
           <properties>
              <markdown>
                 <outputextension>html</outputextension>
                 <sourceextension>md</sourceextension>
              </markdown>   
              <sass>
                <outputextension>css</outputextension>
                <sourceextension>scss</sourceextension>
              </sass>  
              <lesscss>
                <outputextension>css</outputextension>
                <sourceextension>less</sourceextension>
                <convert>
                   <filename>%AppData%\npm\lessc.cmd</filename>
                   <parameters>[%InputFilename%] [%OutputFilename%]</parameters>
                </convert>    
              </lesscss>
           </properties>   
        </WebProcessor>
        <WebServer>
          <filename>WebServer.dll</filename>
          <properties>
              <DefaultDocument>index.html</DefaultDocument>
              <Server>127.0.0.1</Server>
              <Port>8080</Port>
              <UseSSL>false</UseSSL>
              <SSLPassword>aaaa</SSLPassword>
              <SSLKeyFile>sample.key</SSLKeyFile>
              <SSLCertFile>sample.crt</SSLCertFile>
              <SSLRootCertFile>sampleRoot.pem</SSLRootCertFile>
              <404>404.html</404>
          </properties>
        </WebServer>                                 
        <CodeDocsProcessor>
           <filename>CodeDocsProcessor.dll</filename>
           <properties>
              <outputextension>md</outputextension>
           </properties>
        </CodeDocsProcessor>
        <SQLDir>
          <filename>SQLDir.dll</filename>
          <properties>
             <DBSchemaFile>sqldirect.xml</DBSchemaFile>
           </properties>
        </SQLDir>
    </plugins>
</config>