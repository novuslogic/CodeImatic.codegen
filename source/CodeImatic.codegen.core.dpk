{$I CodeImatic.codegen.inc}
package CodeImatic.codegen.core;

{$R *.res}
{$IFDEF IMPLICITBUILDING This IFDEF should not be used by users}
{$ALIGN 8}
{$ASSERTIONS ON}
{$BOOLEVAL OFF}
{$DEBUGINFO OFF}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS ON}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION OFF}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO ON}
{$SAFEDIVIDE OFF}
{$STACKFRAMES ON}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST OFF}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DEFINE DEBUG}
{$ENDIF IMPLICITBUILDING}
{$RUNONLY}
{$IMPLICITBUILD OFF}

requires
  rtl,
  NovusCodeLibrary_Utils,
  NovusCodeLibrary_Object,
  NovusCodeLibrary_Log,
  NovusCodeLibrary_XML,
  NovusCodeLibrary_Parser,
  NovusCodeLibrary_Plugin,
  NovusCodeLibrary_Windows,
  NovusCodeLibrary_OS,
  NovusCodeLibrary_WebUtils,
  NovusCodeLibrary_Console;

contains
  Output in 'Output.pas',
  APIBase in 'Plugins\API\APIBase.pas',
  Runtime in 'Runtime.pas',
  ExpressionParser in 'ExpressionParser.pas',
  Config in 'Config.pas',
  Project in 'Project.pas',
  ConfigProperties in 'ConfigProperties.pas',
  TokenParser in 'TokenParser.pas',
  ProjectParser in 'ProjectParser.pas',
  VariablesCmdLine in 'VariablesCmdLine.pas',
  ProjectItem in 'ProjectItem.pas',
  ProjectItemLoader in 'ProjectItemLoader.pas',
  CodeGeneratorItem in 'CodeGeneratorItem.pas',
  TagType in 'TagType.pas',
  TagParser in 'TagParser.pas',
  Processor in 'Processor.pas',
  Template in 'Template.pas',
  ProjectItemFolder in 'ProjectItemFolder.pas',
  Loader in 'Loader.pas',
  DataProcessor in 'DataProcessor.pas',
  Properties in 'Properties.pas',
  CodeGenerator in 'CodeGenerator.pas',
  Interpreter in 'Interpreter.pas',
  PascalScript in 'PascalScript.pas',
  RuntimeProjectItems in 'RuntimeProjectItems.pas',
  FunctionsParser in 'FunctionsParser.pas',
  TokenProcessor in 'TokenProcessor.pas',
  Variables in 'Variables.pas',
  StatementParser in 'StatementParser.pas',
  TagBasePlugin in 'TagBasePlugin.pas',
  FileExistsFunctionParser in 'FileExistsFunctionParser.pas',
  ProjectConfigLoader in 'ProjectConfigLoader.pas',
  CommandLine in 'CommandLine.pas',
  Plugin in 'Plugin.pas',
  Language in 'Language.pas';

end.

