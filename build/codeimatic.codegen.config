<?xml version="1.0" encoding="utf-8" ?>
<config version="codeimatic.codegen1.0">
    <plugins>
        <SysTags>
          <filename>CodeImatic.codegen.Tags.Sys.dll</filename>
        </SysTags>
         <CodeDocsTags>
          <filename>CodeImatic.codegen.Tags.CodeDocs.dll</filename>
        </CodeDocsTags>
        <DBTags>
          <filename>CodeImatic.codegen.Tags.DB.dll</filename>
        </DBTags>
        <JSONTags>
          <filename>CodeImatic.codegen.Tags.JSON.dll</filename>
        </JSONTags>
        <XMLTags>
          <filename>CodeImatic.codegen.Tags.XML.dll</filename>
        </XMLTags>
        <SystemExt>
          <filename>CodeImatic.codegen.Pascal.SystemExt.dll</filename>
        </SystemExt>
        <WebProcessor>
           <filename>CodeImatic.codegen.Processor.Web.dll</filename>
           <properties>
              <markdown>
                 <outputextension>html</outputextension>
                 <sourceextension>md</sourceextension>
              </markdown>   
              <sass>
                <outputextension>css</outputextension>
                <sourceextension>scss</sourceextension>
              </sass>  
           </properties>   
        </WebProcessor>
        <WebServer>
          <filename>CodeImatic.codegen.WebServer.dll</filename>
          <properties>
              <DefaultDocument>index.html</DefaultDocument>
              <Server>127.0.0.1</Server>
              <Port>8080</Port>
              <UseSSL>True</UseSSL>
              <SSLPassword></SSLPassword>
              <SSLKeyFile>device1.key</SSLKeyFile>
              <SSLCertFile>device1.crt</SSLCertFile>
              <SSLRootCertFile></SSLRootCertFile>
              <404>404.html</404>
          </properties>
        </WebServer>                                 
        <CodeDocsProcessor>
           <filename>CodeImatic.codegen.Processor.CodeDocs.dll</filename>
           <properties>
              <outputextension>md</outputextension>
           </properties>
        </CodeDocsProcessor>
        <SQLDir>
          <filename>CodeImatic.codegen.DataProcessor.SQLDir.dll</filename>
          <properties>
             <DBSchemaFile>sqldirect.xml</DBSchemaFile>
           </properties>
        </SQLDir>
    </plugins>
</config>